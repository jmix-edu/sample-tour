<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<view xmlns="http://jmix.io/schema/flowui/view" xmlns:maps="http://jmix.io/schema/maps/ui"
      title="msg://officePlanView.title">
    <data>
        <collection id="usersDc" class="com.company.tour.entity.User">
            <loader id="usersDl" readOnly="true">
                <query>
                    <![CDATA[select e from User e]]>
                </query>
            </loader>
            <fetchPlan extends="_base"/>
        </collection>
    </data>
    <facets>
        <dataLoadCoordinator auto="true"/>
    </facets>
    <layout>
        <maps:geoMap id="map" width="100%" height="100%">
            <maps:mapView zoom="2" centerX="500" centerY="310">
                <maps:projection code="plan-office" units="PIXELS">
                    <maps:extent minX="0" minY="0" maxX="1000" maxY="621"/>
                </maps:projection>
            </maps:mapView>
            <maps:layers>
                <maps:image>
                    <maps:imageStaticSource url="icons/map/office-plan.jpg">
                        <maps:imageExtent minX="0" minY="0" maxX="1000" maxY="621"/>
                    </maps:imageStaticSource>
                </maps:image>
                <maps:vector id="vector">
                    <maps:vectorSource id="source"/>
                </maps:vector>
                <maps:vector id="dataVector">
                    <maps:dataVectorSource id="dataSource" dataContainer="usersDc" property="workspaceLocation"/>
                </maps:vector>
            </maps:layers>
        </maps:geoMap>
    </layout>
</view>